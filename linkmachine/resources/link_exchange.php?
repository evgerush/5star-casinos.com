//Menu object creation
oCMenu=new makeCM("oCMenu") //Making the menu object. Argument: menuname

//Menu properties   
oCMenu.frames = 0
oCMenu.pxBetween=30
oCMenu.fromLeft=644 
oCMenu.fromTop=39   
oCMenu.rows=1 
oCMenu.menuPlacement=0
oCMenu.offlineRoot="" 
oCMenu.onlineRoot="" 
oCMenu.resizeCheck=1 
oCMenu.wait=500 
oCMenu.fillImg="spacer.gif"
oCMenu.zIndex=0
oCMenu.useBar=0

//Level 0 properties
oCMenu.level[0]=new cm_makeLevel()
oCMenu.level[0].width=60
oCMenu.level[0].height=16 
oCMenu.level[0].regClass="clLevel0"
oCMenu.level[0].overClass="clLevel0over"
oCMenu.level[0].borderX=1
oCMenu.level[0].borderY=1
oCMenu.level[0].borderClass="clLevel0border"
oCMenu.level[0].offsetX=0
oCMenu.level[0].offsetY=0
oCMenu.level[0].rows=0
oCMenu.level[0].arrow=0
oCMenu.level[0].arrowWidth=0
oCMenu.level[0].arrowHeight=0
oCMenu.level[0].align="bottomleft"

// Layer 1 properties
oCMenu.level[1]=new cm_makeLevel()
oCMenu.level[1].width=110
oCMenu.level[1].height=20
oCMenu.level[1].regClass="clLevel1"
oCMenu.level[1].overClass="clLevel1over"
oCMenu.level[1].borderX=1
oCMenu.level[1].borderY=1
oCMenu.level[1].align="right" 
oCMenu.level[1].offsetX=-(oCMenu.level[0].width-2)/2+20
oCMenu.level[1].offsetY=0
oCMenu.level[1].borderClass="clLevel1border"

oCMenu.makeMenu('top0','','&nbsp;&nbsp;Go To...&nbsp;','','')
  oCMenu.makeMenu('sub00','top0','Link Manager','linkmachine.php')
	oCMenu.makeMenu('sub01','top0','SEO Wizard','linkmachine.php?ua=_ua130')
